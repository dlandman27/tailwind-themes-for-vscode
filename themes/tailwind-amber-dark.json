{
  "name": "Tailwind Amber Dark",
  "type": "dark",
  "colors": {
    "editor.background": "#451a03",
    "editor.foreground": "#fef3c7",
    "editorCursor.foreground": "#fbbf24",
    "editor.lineHighlightBackground": "#92400e",
    "editor.selectionBackground": "#b45309",
    "editor.selectionHighlightBackground": "#b45309",
    "editor.wordHighlightBackground": "#b45309",
    "editor.wordHighlightStrongBackground": "#b45309",
    "editor.findMatchBackground": "#d97706",
    "editor.findMatchHighlightBackground": "#b45309",
    "editor.hoverHighlightBackground": "#92400e",
    "editorLineNumber.foreground": "#f59e0b",
    "editorLineNumber.activeForeground": "#fcd34d",
    "editorIndentGuide.background": "#b45309",
    "editorIndentGuide.activeBackground": "#f59e0b",
    "editorWhitespace.foreground": "#d97706",
    "editorRuler.foreground": "#b45309",
    "activityBar.background": "#92400e",
    "activityBar.foreground": "#fde68a",
    "activityBar.activeBorder": "#fbbf24",
    "activityBarBadge.background": "#f59e0b",
    "activityBarBadge.foreground": "#78350f",
    "sideBar.background": "#92400e",
    "sideBar.foreground": "#fde68a",
    "sideBarTitle.foreground": "#fef3c7",
    "sideBarSectionHeader.background": "#b45309",
    "sideBarSectionHeader.foreground": "#fef3c7",
    "statusBar.background": "#b45309",
    "statusBar.foreground": "#fef3c7",
    "statusBar.debuggingBackground": "#b91c1c",
    "statusBar.debuggingForeground": "#fee2e2",
    "statusBar.noFolderBackground": "#d97706",
    "tab.activeBackground": "#451a03",
    "tab.activeForeground": "#fef3c7",
    "tab.inactiveBackground": "#92400e",
    "tab.inactiveForeground": "#fbbf24",
    "tab.border": "#b45309",
    "tab.activeBorder": "#fbbf24",
    "panel.background": "#92400e",
    "panel.border": "#b45309",
    "panelTitle.activeForeground": "#fef3c7",
    "panelTitle.inactiveForeground": "#fbbf24",
    "terminal.background": "#451a03",
    "terminal.foreground": "#fef3c7",
    "terminal.ansiBlack": "#92400e",
    "terminal.ansiRed": "#f87171",
    "terminal.ansiGreen": "#4ade80",
    "terminal.ansiYellow": "#facc15",
    "terminal.ansiBlue": "#60a5fa",
    "terminal.ansiMagenta": "#c084fc",
    "terminal.ansiCyan": "#22d3ee",
    "terminal.ansiWhite": "#fde68a",
    "button.background": "#d97706",
    "button.foreground": "#fef3c7",
    "button.hoverBackground": "#f59e0b",
    "input.background": "#92400e",
    "input.foreground": "#fef3c7",
    "input.border": "#d97706",
    "inputOption.activeBorder": "#fbbf24",
    "dropdown.background": "#92400e",
    "dropdown.foreground": "#fef3c7",
    "dropdown.border": "#d97706",
    "list.activeSelectionBackground": "#b45309",
    "list.activeSelectionForeground": "#fef3c7",
    "list.hoverBackground": "#92400e",
    "list.inactiveSelectionBackground": "#92400e",
    "list.focusBackground": "#b45309",
    "scrollbar.shadow": "#451a03",
    "scrollbarSlider.background": "#d97706",
    "scrollbarSlider.hoverBackground": "#f59e0b",
    "scrollbarSlider.activeBackground": "#fbbf24",
    "badge.background": "#f59e0b",
    "badge.foreground": "#78350f",
    "progressBar.background": "#f59e0b",
    "window.activeBorder": "#d97706",
    "window.inactiveBorder": "#b45309",
    "titleBar.activeBackground": "#b45309",
    "titleBar.activeForeground": "#fef3c7",
    "titleBar.inactiveBackground": "#92400e",
    "titleBar.inactiveForeground": "#fbbf24",
    "menuBar.selectionBackground": "#d97706",
    "menuBar.selectionForeground": "#fef3c7",
    "editorGroupHeader.tabsBackground": "#92400e"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#fbbf24"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#fde68a"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#c084fc"
      }
    },
    {
      "name": "Operators",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#f472b6"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.quoted"
      ],
      "settings": {
        "foreground": "#4ade80"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "meta.require",
        "support.function.any-method"
      ],
      "settings": {
        "foreground": "#60a5fa"
      }
    },
    {
      "name": "Classes",
      "scope": [
        "entity.name.type",
        "entity.other.inherited-class",
        "support.class"
      ],
      "settings": {
        "foreground": "#facc15"
      }
    },
    {
      "name": "Numbers",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#fb923c"
      }
    },
    {
      "name": "Tags",
      "scope": [
        "entity.name.tag",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#f87171"
      }
    },
    {
      "name": "Attributes",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#fb923c"
      }
    }
  ]
}