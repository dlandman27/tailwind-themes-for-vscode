{
  "name": "Tailwind Rose Light",
  "type": "light",
  "colors": {
    "editor.background": "#fff1f2",
    "editor.foreground": "#9f1239",
    "editorCursor.foreground": "#e11d48",
    "editor.lineHighlightBackground": "#ffe4e6",
    "editor.selectionBackground": "#fecdd3",
    "editor.selectionHighlightBackground": "#fecdd3",
    "editor.wordHighlightBackground": "#fecdd3",
    "editor.wordHighlightStrongBackground": "#fecdd3",
    "editor.findMatchBackground": "#fda4af",
    "editor.findMatchHighlightBackground": "#fecdd3",
    "editor.hoverHighlightBackground": "#ffe4e6",
    "editorLineNumber.foreground": "#fb7185",
    "editorLineNumber.activeForeground": "#e11d48",
    "editorIndentGuide.background": "#fecdd3",
    "editorIndentGuide.activeBackground": "#fb7185",
    "editorWhitespace.foreground": "#fda4af",
    "editorRuler.foreground": "#fecdd3",
    "activityBar.background": "#ffe4e6",
    "activityBar.foreground": "#be123c",
    "activityBar.activeBorder": "#f43f5e",
    "activityBarBadge.background": "#f43f5e",
    "activityBarBadge.foreground": "#fff1f2",
    "sideBar.background": "#ffe4e6",
    "sideBar.foreground": "#be123c",
    "sideBarTitle.foreground": "#9f1239",
    "sideBarSectionHeader.background": "#fecdd3",
    "sideBarSectionHeader.foreground": "#9f1239",
    "statusBar.background": "#e11d48",
    "statusBar.foreground": "#fff1f2",
    "statusBar.debuggingBackground": "#dc2626",
    "statusBar.debuggingForeground": "#fef2f2",
    "statusBar.noFolderBackground": "#f43f5e",
    "tab.activeBackground": "#fff1f2",
    "tab.activeForeground": "#9f1239",
    "tab.inactiveBackground": "#ffe4e6",
    "tab.inactiveForeground": "#e11d48",
    "tab.border": "#fecdd3",
    "tab.activeBorder": "#f43f5e",
    "panel.background": "#ffe4e6",
    "panel.border": "#fecdd3",
    "panelTitle.activeForeground": "#9f1239",
    "panelTitle.inactiveForeground": "#e11d48",
    "terminal.background": "#fff1f2",
    "terminal.foreground": "#9f1239",
    "terminal.ansiBlack": "#881337",
    "terminal.ansiRed": "#ef4444",
    "terminal.ansiGreen": "#22c55e",
    "terminal.ansiYellow": "#eab308",
    "terminal.ansiBlue": "#3b82f6",
    "terminal.ansiMagenta": "#a855f7",
    "terminal.ansiCyan": "#06b6d4",
    "terminal.ansiWhite": "#ffe4e6",
    "button.background": "#f43f5e",
    "button.foreground": "#fff1f2",
    "button.hoverBackground": "#e11d48",
    "input.background": "#fff1f2",
    "input.foreground": "#9f1239",
    "input.border": "#fda4af",
    "inputOption.activeBorder": "#f43f5e",
    "dropdown.background": "#fff1f2",
    "dropdown.foreground": "#9f1239",
    "dropdown.border": "#fda4af",
    "list.activeSelectionBackground": "#fecdd3",
    "list.activeSelectionForeground": "#9f1239",
    "list.hoverBackground": "#ffe4e6",
    "list.inactiveSelectionBackground": "#ffe4e6",
    "list.focusBackground": "#fecdd3",
    "scrollbar.shadow": "#fecdd3",
    "scrollbarSlider.background": "#fda4af",
    "scrollbarSlider.hoverBackground": "#fb7185",
    "scrollbarSlider.activeBackground": "#f43f5e",
    "badge.background": "#f43f5e",
    "badge.foreground": "#fff1f2",
    "progressBar.background": "#f43f5e",
    "window.activeBorder": "#fda4af",
    "window.inactiveBorder": "#fecdd3"
  },
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#f43f5e"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "string constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#be123c"
      }
    },
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#9333ea"
      }
    },
    {
      "name": "Operators",
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#db2777"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.quoted"
      ],
      "settings": {
        "foreground": "#16a34a"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "meta.require",
        "support.function.any-method"
      ],
      "settings": {
        "foreground": "#2563eb"
      }
    },
    {
      "name": "Classes",
      "scope": [
        "entity.name.type",
        "entity.other.inherited-class",
        "support.class"
      ],
      "settings": {
        "foreground": "#a16207"
      }
    },
    {
      "name": "Numbers",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character.escape"
      ],
      "settings": {
        "foreground": "#ea580c"
      }
    },
    {
      "name": "Tags",
      "scope": [
        "entity.name.tag",
        "markup.deleted.git_gutter"
      ],
      "settings": {
        "foreground": "#dc2626"
      }
    },
    {
      "name": "Attributes",
      "scope": [
        "entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#ea580c"
      }
    }
  ]
}